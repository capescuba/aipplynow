{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:PutObjectAcl",
                "s3:GetObject",
                "s3:DeleteObject"
            ],
            "Resource": "arn:aws:s3:::aipplynow-resumes/*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetBucketEncryption",
                "s3:GetBucketPolicy"
            ],
            "Resource": "arn:aws:s3:::aipplynow-resumes"
        }
    ]
}